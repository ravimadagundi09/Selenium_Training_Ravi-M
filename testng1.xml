<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Suite" thread-count="5" parallel="tests">
  <test name="Chrome">
        <parameter name="os" value="Windows" />
        <parameter name="osVersion" value= "10" />
        <parameter name="browser" value="Chrome" />
        <parameter name="browserVersion" value= "105.0"/>
        <classes>
            <class name="day5.SeleniumGrid1" />
        </classes>
    </test> <!-- Test -->
    <test name="Firefox">
        <parameter name="os" value="Windows"/>
        <parameter name="osVersion" value="10" />
        <parameter name="browser" value="Firefox" />
        <parameter name="browserVersion" value="latest-beta" />
        <classes>
            <class name="day5.SeleniumGrid1" />
        </classes>
    </test> <!-- Test -->
    <test name="Edge">
        <parameter name="os" value="Windows" />
        <parameter name="osVersion" value="10" />
        <parameter name="browser" value="Edge" />
        <parameter name="browserVersion" value="100.0" />
        <classes>
            <class name="day5.SeleniumGrid1" />
        </classes>
    </test> <!-- Test -->
    <test name="MacChrome">
        <parameter name="os" value="OS X" />
        <parameter name="osVersion" value="Ventura" />
        <parameter name="browser" value="Chrome" />
        <parameter name="browserVersion" value="105.0" />
        <classes>
            <class name="day5.SeleniumGrid1" />
        </classes>
    </test> <!-- Test -->
    <test name="MacFirefox">
        <parameter name="os" value="OS X"/>
        <parameter name="osVersion" value="Ventura" />
        <parameter name="browser" value="Firefox" />
        <parameter name="browserVersion" value="100.0" />
        <classes>
            <class name="day5.SeleniumGrid1" />
        </classes>
    </test> <!-- Test -->


</suite> <!-- Suite -->
